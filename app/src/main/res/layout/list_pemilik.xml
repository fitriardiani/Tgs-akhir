<?xml version="1.0" encoding="utf-8"?>
<TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:tools="http://schemas.android.com/tools"
    android:background="@android:drawable/editbox_background_normal"
    android:padding="30dp"
    android:orientation="vertical">

    <TableRow
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <TextView
            android:id="@+id/textView18"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Id Rfid"
            tools:layout_editor_absoluteY="21dp" />

        <TextView
            android:id="@+id/textView17"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=":"
            tools:layout_editor_absoluteY="19dp"
            android:layout_marginRight="17dp"
            android:layout_marginLeft="15dp" />
        <TextView
            android:id="@+id/txt_id_rfid"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@android:color/black" />

    </TableRow>
    <TableRow
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <TextView
            android:id="@+id/textView20"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Tgl Memiliki"
            tools:layout_editor_absoluteY="21dp" />

        <TextView
            android:id="@+id/textView21"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=":"
            tools:layout_editor_absoluteY="19dp"
            android:layout_marginRight="17dp"
            android:layout_marginLeft="15dp" />
        <TextView
            android:id="@+id/txt_tgl_memiliki"
            android:layout_below="@id/txt_id_rfid"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@android:color/black" />

    </TableRow>
    <TableRow
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <TextView
            android:id="@+id/textView22"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Nama Pemilik"
            tools:layout_editor_absoluteY="21dp" />

        <TextView
            android:id="@+id/textView23"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=":"
            tools:layout_editor_absoluteY="19dp"
            android:layout_marginRight="17dp"
            android:layout_marginLeft="15dp" />
        <TextView
            android:id="@+id/txt_nama_pemilik"
            android:layout_below="@id/txt_tgl_memiliki"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@android:color/black" />

    </TableRow>
    <TableRow
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <TextView
            android:id="@+id/textView24"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Pemilik Ke-"
            tools:layout_editor_absoluteY="21dp" />

        <TextView
            android:id="@+id/textView25"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=":"
            tools:layout_editor_absoluteY="19dp"
            android:layout_marginRight="17dp"
            android:layout_marginLeft="15dp" />
        <TextView
            android:id="@+id/txt_pemilikke"
            android:layout_below="@id/txt_nama_pemilik"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@android:color/black" />

    </TableRow>
    <TableRow
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <TextView
            android:id="@+id/textView15"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Alamat"
            tools:layout_editor_absoluteY="21dp" />

        <TextView
            android:id="@+id/textView11"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=":"
            tools:layout_editor_absoluteY="19dp"
            android:layout_marginRight="17dp"
            android:layout_marginLeft="15dp" />
        <TextView
            android:id="@+id/txt_alamat"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@android:color/black" />

    </TableRow>


</TableLayout>